import { Injector, ViewContainerRef, TemplateRef, OnDestroy } from "@angular/core";
import { RxTranslateConfig } from "../interface/rx-translate-config";
import { ActivatedRoute } from "@angular/router";
import { HttpClient } from "@angular/common/http";
export declare class RxTranslateDirective implements OnDestroy {
    private viewContainerRef;
    private templateRef;
    private injector;
    private route;
    private httpClient;
    private viewRef;
    private _context;
    private config;
    private component;
    private subscription;
    private baseConfig;
    constructor(viewContainerRef: ViewContainerRef, templateRef: TemplateRef<any>, injector: Injector, baseConfig: RxTranslateConfig, route: ActivatedRoute, httpClient: HttpClient);
    set translate(value: any);
    private updateView;
    ngOnDestroy(): void;
}
